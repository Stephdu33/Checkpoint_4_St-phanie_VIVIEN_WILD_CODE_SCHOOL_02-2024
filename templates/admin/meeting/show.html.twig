{% extends 'admin.html.twig' %}

{% block title %}Meeting
{% endblock %}

{% block body %}
	<h1>Meeting</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ meeting.id }}</td>
			</tr>
			<tr>
				<th>Date</th>
				<td>{{ meeting.date ? meeting.date|date('Y-m-d') : '' }}</td>
			</tr>
			<tr>
				<th>Hour</th>
				<td>{{ meeting.hour ? meeting.hour|date('H:i:s') : '' }}</td>
			</tr>
		</tbody>
	</table>

	<a href="{{ path('app_admin_meeting_index') }}">back to list</a>

	<a href="{{ path('app_admin_meeting_edit', {'id': meeting.id}) }}">edit</a>

	{{ include('admin/meeting/_delete_form.html.twig') }}
{% endblock %}
