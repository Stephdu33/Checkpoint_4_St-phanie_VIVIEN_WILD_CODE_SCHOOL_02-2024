{% extends 'admin.html.twig' %}

{% block title %}Hello Steph!
{% endblock %}

{% block body %}
	<div class="block">
		<div class="container-experience">

			<h1 class="h1-experiences">Ajoute une nouvelle exp√©rience professionnelle</h1>
			<div class="form-new-experience-block">
				{{ form_start(formExperience) }}

				<div class="form-new-experience-company">
					{{ form_widget(formExperience.company) }}
					<div class="form-experience-error">
						{{ form_errors(formExperience.company) }}
					</div>
				</div>

				<div class="form-new-experience-job">
					{{ form_widget(formExperience.job) }}
				</div>

				<div class="form-new-experience-startyear">
					{{ form_widget(formExperience.startyear) }}
				</div>

				<div class="form-new-experience-endyear">
					{{ form_widget(formExperience.endyear) }}
				</div>

				{{ form_row(formExperience.submit) }}

				{{ form_end(formExperience) }}
			</div>
			<div class="link-come-back">
				<a href="{{ path('app_admin_experience_index') }}" class="link-come-back-button">Retour</a>
				<svg xmlns="http://www.w3.org/2000/svg" viewbox="0 0 512 512" width="18px" class="icons"><!--!Font Awesome Free 6.5.1 by @fontawesome - https://fontawesome.com License - https://fontawesome.com/license/free Copyright 2024 Fonticons, Inc.--><path d="M48.5 224H40c-13.3 0-24-10.7-24-24V72c0-9.7 5.8-18.5 14.8-22.2s19.3-1.7 26.2 5.2L98.6 96.6c87.6-86.5 228.7-86.2 315.8 1c87.5 87.5 87.5 229.3 0 316.8s-229.3 87.5-316.8 0c-12.5-12.5-12.5-32.8 0-45.3s32.8-12.5 45.3 0c62.5 62.5 163.8 62.5 226.3 0s62.5-163.8 0-226.3c-62.2-62.2-162.7-62.5-225.3-1L185 183c6.9 6.9 8.9 17.2 5.2 26.2s-12.5 14.8-22.2 14.8H48.5z"/></svg>
			</div>

		</div>
	</div>
{% endblock %}
