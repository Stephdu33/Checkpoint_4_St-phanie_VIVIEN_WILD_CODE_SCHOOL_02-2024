{% extends 'admin.html.twig' %}

{% block title %}Experience
{% endblock %}

{% block body %}
	<h1>Experience</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ experience.id }}</td>
			</tr>
			<tr>
				<th>Company</th>
				<td>{{ experience.company }}</td>
			</tr>
			<tr>
				<th>Job</th>
				<td>{{ experience.job }}</td>
			</tr>
			<tr>
				<th>Startyear</th>
				<td>{{ experience.startyear ? experience.startyear|date('Y-m-d') : '' }}</td>
			</tr>
			<tr>
				<th>Endyear</th>
				<td>{{ experience.endyear ? experience.endyear|date('Y-m-d') : '' }}</td>
			</tr>
		</tbody>
	</table>

	<a href="{{ path('app_admin_experience_index') }}">back to list</a>

	<a href="{{ path('app_admin_experience_edit', {'id': experience.id}) }}">edit</a>

	{{ include('admin/experience/_delete_form.html.twig') }}
{% endblock %}
