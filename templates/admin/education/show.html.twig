{% extends 'admin.html.twig' %}

{% block title %}Education
{% endblock %}

{% block body %}
	<h1>Education</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ education.id }}</td>
			</tr>
			<tr>
				<th>Title</th>
				<td>{{ education.title }}</td>
			</tr>
			<tr>
				<th>Description</th>
				<td>{{ education.description }}</td>
			</tr>
			<tr>
				<th>Year</th>
				<td>{{ education.year ? education.year|date('Y-m-d') : '' }}</td>
			</tr>
			<tr>
				<th>School</th>
				<td>{{ education.school }}</td>
			</tr>
		</tbody>
	</table>

	<a href="{{ path('app_admin_education_index') }}">back to list</a>

	<a href="{{ path('app_admin_education_edit', {'id': education.id}) }}">edit</a>

	{{ include('admin/education/_delete_form.html.twig') }}
{% endblock %}
